.taskDanCheck {
  padding: 32rpx;
  background-color: #F2F3F8;
}
.taskDanCheck .taskDanCheckNav {
  line-height: 128rpx;
  margin: 0 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  text-align: center;
  color: #626A74;
}
.taskDanCheck .taskDanCheckNav .taskDanCheckNavItem {
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
}
.taskDanCheck .taskDanCheckNav .taskDanCheckNavItem .CheckNavSeHorn {
  width: 64rpx;
  height: 6rpx;
  margin: 0 auto;
  background-color: #025FFD;
  border-radius: 2rpx;
}
.taskDanCheck .taskDanCheckNav .CheckNavSe {
  color: #025FFD;
  font-weight: 500;
}
.taskDanCheck .taskDanCheckMain {
  margin-top: 24rpx;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem {
  position: relative;
  font-size: 26rpx;
  margin-bottom: 24rpx;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemTime {
  color: #888;
  margin-bottom: 16rpx;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemB {
  line-height: 54rpx;
  padding: 24rpx;
  background-color: #fff;
  border-radius: 8rpx;
  border-left: 8rpx solid #55AA8F;
  display: flex;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemB .taskCheckItemBL {
  flex: 1;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemB .taskCheckItemBL .taskCheckItemBLTitle {
  font-size: 30rpx;
  font-weight: 600;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemB .taskCheckItemBR {
  width: 120rpx;
  text-align: center;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemB .taskCheckItemBR .taskCheckItemBRIcon {
  width: 24rpx;
  height: 24rpx;
  margin: 12rpx auto;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemChild {
  color: #666;
  padding: 24rpx;
  background-color: #fff;
  box-shadow: 0 0 4rpx #999;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemChild .taskCheckItemChildItem {
  padding: 8px 0;
  border-bottom: 1px solid #999;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemChild .taskCheckItemChildItem .taskCheckItemChildItemT {
  line-height: 64rpx;
  display: flex;
  justify-content: space-between;
}
.taskDanCheck .taskDanCheckMain .taskDanCheckItem .taskCheckItemChild .taskCheckItemChildItem .taskCheckItemChildItemT .taskCheckItemChildItemTS {
  color: #568BD9;
}
.taskDanCheck .taskDanCheckMain .taskCheckItemBtn {
  height: 26px;
  line-height: 26px;
  margin-top: 6px;
  display: inline-block;
  padding: 4rpx 36rpx;
  background: #2979FF;
  color: #fff;
  border-radius: 12rpx;
}